<?xml version="1.0" encoding="UTF-8"?>
<assessmentItem xmlns="http://www.imsglobal.org/xsd/imsqti_v2p1"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.imsglobal.org/xsd/imsqti_v2p1 http://www.imsglobal.org/xsd/imsqti_v2p1.xsd"
	identifier="matchTowns" title="Towns" adaptive="false" timeDependent="false">
	
	<responseDeclaration identifier="RESPONSE" cardinality="multiple" baseType="directedPair">
		<mapping defaultValue="0">
			<mapEntry mapKey="B NTS" mappedValue="1"/>
			<mapEntry mapKey="S Y"   mappedValue="1"/>
			<mapEntry mapKey="M D"   mappedValue="1"/>
			<mapEntry mapKey="H Y"   mappedValue="1"/>
			<mapEntry mapKey="N NTH" mappedValue="1"/>
			<mapEntry mapKey="N S"   mappedValue="1"/>
			<mapEntry mapKey="B NTH" mappedValue="1"/>

			<mapEntry mapKey="H NTH" mappedValue="-0.5"/>
			<mapEntry mapKey="H S"   mappedValue="-0.5"/>
			<mapEntry mapKey="H NTS" mappedValue="-0.5"/>
			<mapEntry mapKey="H D"   mappedValue="-0.5"/>
			<mapEntry mapKey="B Y"   mappedValue="-0.5"/>
			<mapEntry mapKey="B S"   mappedValue="-0.5"/>
			<mapEntry mapKey="B D"   mappedValue="-0.5"/>
			<mapEntry mapKey="S NTH" mappedValue="-0.5"/>
			<mapEntry mapKey="S S"   mappedValue="-0.5"/>
			<mapEntry mapKey="S NTS" mappedValue="-0.5"/>
			<mapEntry mapKey="S D"   mappedValue="-0.5"/>
			<mapEntry mapKey="N Y"   mappedValue="-0.5"/>
			<mapEntry mapKey="N NTS" mappedValue="-0.5"/>
			<mapEntry mapKey="N D"   mappedValue="-0.5"/>
			<mapEntry mapKey="M NTH" mappedValue="-0.5"/>
			<mapEntry mapKey="M Y"   mappedValue="-0.5"/>
			<mapEntry mapKey="M S"   mappedValue="-0.5"/>
			<mapEntry mapKey="M NTS" mappedValue="-0.5"/>
		</mapping>
	
		<correctResponse>
			<value>B NTH</value>
			<value>B NTS</value>
			<value>N S</value>
			<value>N NTH</value>
			<value>M D</value>
			<value>H Y</value>
			<value>S Y</value>
		</correctResponse>
	</responseDeclaration>
	
	<outcomeDeclaration identifier="FEEDBACK" cardinality="single" baseType="identifier"/>
	<outcomeDeclaration identifier="SCORE" cardinality="single" baseType="float"/>
	<outcomeDeclaration identifier="ALLCORRECT" cardinality="single" baseType="float"/>
	<outcomeDeclaration identifier="SOMEWRONG" cardinality="single" baseType="float"/>
	<itemBody>
		<matchInteraction responseIdentifier="RESPONSE" shuffle="true" maxAssociations="4">
			<prompt>Match the following towns to the county they appear in:</prompt>
			<simpleMatchSet>
				<simpleAssociableChoice identifier="B" matchMax="5">Blyth</simpleAssociableChoice>
				<simpleAssociableChoice identifier="N" matchMax="5">Newcastle</simpleAssociableChoice>
				<simpleAssociableChoice identifier="H" matchMax="5">Harrogate</simpleAssociableChoice>
				<simpleAssociableChoice identifier="M" matchMax="5">Matlock</simpleAssociableChoice>
				<simpleAssociableChoice identifier="S" matchMax="5">Skipton</simpleAssociableChoice>
			</simpleMatchSet>
			<simpleMatchSet>
				<simpleAssociableChoice identifier="NTH" matchMax="5">Northumberland</simpleAssociableChoice>
				<simpleAssociableChoice identifier="Y" matchMax="5">Yorkshire</simpleAssociableChoice>
				<simpleAssociableChoice identifier="S" matchMax="5">Staffordshire</simpleAssociableChoice>
				<simpleAssociableChoice identifier="NTS" matchMax="5">Nottinghamshire</simpleAssociableChoice>
				<simpleAssociableChoice identifier="D" matchMax="5">Derbyshire</simpleAssociableChoice>
			</simpleMatchSet>
		</matchInteraction>
	</itemBody>

<responseProcessing>
	<setOutcomeValue identifier="SCORE">
                <mapResponse identifier="RESPONSE"/>
        </setOutcomeValue>
        <responseCondition>
		<responseIf>
			<and>
				<member>
					<baseValue baseType="pair">B NTH</baseValue>
					<variable identifier="RESPONSE"/>
				</member>
				<member>
					<baseValue baseType="pair">B NTS</baseValue>
					<variable identifier="RESPONSE"/>
				</member>
				<member>
					<baseValue baseType="pair">N S</baseValue>
					<variable identifier="RESPONSE"/>
				</member>
				<member>
					<baseValue baseType="pair">N NTH</baseValue>
					<variable identifier="RESPONSE"/>
				</member>
				<member>
					<baseValue baseType="pair">M D</baseValue>
					<variable identifier="RESPONSE"/>
				</member>
				<member>
					<baseValue baseType="pair">H Y</baseValue>
					<variable identifier="RESPONSE"/>
				</member>
				<member>
					<baseValue baseType="pair">S Y</baseValue>
					<variable identifier="RESPONSE"/>
				</member>
			</and>
			<setOutcomeValue identifier="ALLCORRECT">
				<baseValue baseType="boolean">true</baseValue>
			</setOutcomeValue>
		</responseIf>
		<responseElse>
			<setOutcomeValue identifier="ALLCORRECT">
				<baseValue baseType="boolean">false</baseValue>
			</setOutcomeValue>
		</responseElse>		
          </responseCondition>				
				
	<responseCondition>
		<responseIf>
			<or>
				<member>
					<baseValue baseType="pair">B Y</baseValue>
						<variable identifier="RESPONSE"/>
					</member>
					<member>
						<baseValue baseType="pair">B S</baseValue>
						<variable identifier="RESPONSE"/>
					</member>
					<member>
						<baseValue baseType="pair">B D</baseValue>
						<variable identifier="RESPONSE"/>
					</member>
					<member>
						<baseValue baseType="pair">S NTH</baseValue>
						<variable identifier="RESPONSE"/>
					</member>
					<member>
						<baseValue baseType="pair">S S</baseValue>
						<variable identifier="RESPONSE"/>
					</member>
					<member>
						<baseValue baseType="pair">S NTS</baseValue>
						<variable identifier="RESPONSE"/>
					</member>
					<member>
						<baseValue baseType="pair">S D</baseValue>
						<variable identifier="RESPONSE"/>
					</member>
					<member>
						<baseValue baseType="pair">M NTH</baseValue>
						<variable identifier="RESPONSE"/>
					</member>
					<member>
						<baseValue baseType="pair">M Y</baseValue>
						<variable identifier="RESPONSE"/>
					</member>
					<member>
						<baseValue baseType="pair">M S</baseValue>
						<variable identifier="RESPONSE"/>
					</member>
					<member>
						<baseValue baseType="pair">M NTS</baseValue>
						<variable identifier="RESPONSE"/>
					</member>
					<member>
						<baseValue baseType="pair">H NTH</baseValue>
						<variable identifier="RESPONSE"/>
					</member>
					<member>
						<baseValue baseType="pair">H S</baseValue>
						<variable identifier="RESPONSE"/>
					</member>
					<member>
						<baseValue baseType="pair">H NTS</baseValue>
						<variable identifier="RESPONSE"/>
					</member>
					<member>
						<baseValue baseType="pair">H D</baseValue>
						<variable identifier="RESPONSE"/>
					</member>
					<member>
						<baseValue baseType="pair"> N Y</baseValue>
						<variable identifier="RESPONSE"/>
					</member>
					<member>
						<baseValue baseType="pair">N NTS</baseValue>
						<variable identifier="RESPONSE"/>
					</member>
					<member>
						<baseValue baseType="pair">N D</baseValue>
						<variable identifier="RESPONSE"/>
					</member>
				</or>
			<setOutcomeValue identifier="SOMEWRONG">
				<baseValue baseType="boolean">true</baseValue>
			</setOutcomeValue>
		</responseIf>
		<responseElse>
			<setOutcomeValue identifier="SOMEWRONG">
				<baseValue baseType="boolean">false</baseValue>
			</setOutcomeValue>
		</responseElse>		
          </responseCondition>
          <responseCondition>
          	<responseIf>
          		<and>
          			<variable identifier="ALLCORRECT"/>
          			<not>
          				<variable identifier="SOMEWRONG"/>
          			</not>
          		</and>
          		<setOutcomeValue identifier="FEEDBACK">
				<multiple>
				<baseValue baseType="identifier">correct</baseValue>
				<baseValue baseType="identifier">score</baseValue>
				</multiple>
			</setOutcomeValue>
		</responseIf>
		
		<responseElseIf>
      			<and>
          			<variable identifier="ALLCORRECT"/>
          			<variable identifier="SOMEWRONG"/>
          		</and>
          		<setOutcomeValue identifier="FEEDBACK">
				<multiple>
					<baseValue baseType="identifier">correctsomewrong</baseValue>
					<baseValue baseType="identifier">common</baseValue>
					<baseValue baseType="identifier">score</baseValue>
				</multiple>
			</setOutcomeValue>
		</responseElseIf>
		<responseElseIf>
      			<and>
          			<not>
          			<variable identifier="ALLCORRECT"/>
          			</not>
          			<variable identifier="SOMEWRONG"/>
          		</and>
          		<setOutcomeValue identifier="FEEDBACK">
				<multiple>
					<baseValue baseType="identifier">incorrectsomewrong</baseValue>
					<baseValue baseType="identifier">common</baseValue>
					<baseValue baseType="identifier">score</baseValue>
				</multiple>
			</setOutcomeValue>
		</responseElseIf>
		<responseElse>
			<and>
          			<not>
          			<variable identifier="ALLCORRECT"/>
          			</not>
          			<not>
          			<variable identifier="SOMEWRONG"/>
          			</not>
          		</and>
          		<setOutcomeValue identifier="FEEDBACK">
				<multiple>
					<baseValue baseType="identifier">incorrectnonewrong</baseValue>
					<baseValue baseType="identifier">common</baseValue>
					<baseValue baseType="identifier">score</baseValue>
				</multiple>
			</setOutcomeValue>
          	</responseElse>
	</responseCondition>
</responseProcessing>

<modalFeedback outcomeIdentifier="FEEDBACK" showHide="show" identifier="correct">
	<p>Yes. You have correctly matched all, and made no false matches</p>
</modalFeedback>
<modalFeedback outcomeIdentifier="FEEDBACK" showHide="show" identifier="correctsomewrong">
	<p>No. You have correctly matched all, but you have included some false matches.
	</p>
</modalFeedback>
<modalFeedback outcomeIdentifier="FEEDBACK" showHide="show" identifier="incorrectsomewrong">
	<p>No. You have missed some correct matches, and have included some false matches.
	</p>
</modalFeedback>
<modalFeedback outcomeIdentifier="FEEDBACK" showHide="show" identifier="incorrectnonewrong">
	<p>No.  You have made no false matches, but you have missed some correct matches.
	</p>
</modalFeedback>
<modalFeedback outcomeIdentifier="FEEDBACK" showHide="show" identifier="score">
	<p>You have scored <printedVariable identifier = "SCORE"/> marks.
	</p>
</modalFeedback>
<modalFeedback outcomeIdentifier="FEEDBACK" showHide="show" identifier="common">
	<p>The answers are as follows:<br/>
	<br/>
	Blyth is in Nottinghamshire and Northumberland.<br/>
 	Newcastle is in Northumberland and Staffordshire.<br/>
	Harrogate and Skipton are in Yorkshire.<br/>
	Matlock is in Derbyshire.<br/>
	</p>
</modalFeedback>
</assessmentItem>
